		"obj": true,
		"compiler": true,
		"compiler.exe*": true
	},
	"files.eol": "\n",
	"doxdocgen.generic.briefTemplate": "{text}",
	"doxdocgen.file.fileOrder": [
		"file",
		"brief",
		"empty",
		"empty",
		"version",
		"date",
	],
